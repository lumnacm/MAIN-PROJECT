<div class="page-content-wrapper main-content">
    <div class="container-md">
        <h4 class="mt-3 mb-3">Manage Your Users</h4>
        <div class="card">
            <div class="card-body">
                <table class="w-100" id="dataTable">
                    <thead>
                        <tr>
                            <th>No.</th>
                            <th>Name</th>
                            <th>Email</th>
                            <th>Phone</th>
                            <th>Address</th>
                            <th>District</th>
                            {{!-- <th>Manage</th> --}}
                        </tr>
                    </thead>
                    <tbody>
                        {{#each orders}}
                        <tr>
                            <td>{{incremented @index}}</td>
                            <td>{{this.user.Fname}} {{this.user.Lname}}</td>
                            <td>{{this.user.Email}}</td>
                            <td>{{this.user.Phone}}</td>
                            <td>{{this.user.Address}}</td>
                            <td>{{this.user.District}}</td>
                            {{!-- <td><a href="" class="btn btn-danger btn-sm"><i class="bi bi-trash"></i></a></td> --}}
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>